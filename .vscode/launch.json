{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "ðŸš€ Launch Chrome (Dev)",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "userDataDir": true
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "ðŸ”— Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src"
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "ðŸ¦Š Launch Firefox (Dev)",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src",
      "reAttach": true,
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "type": "edge",
      "request": "launch",
      "name": "ðŸŒŠ Launch Edge (Dev)",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ],
  "compounds": []
}
